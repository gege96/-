#include "stdio.h"

void bubble_sort(int arr[], int len) {
 int i, j, temp;
 for(i = 0; i < len - 1; i++)
   for(j = 0; j < len - 1 - i; j++)
	    if(arr[j] > arr[j + 1]) {
			   temp = arr[j];
			   arr[j] = arr[j + 1];
			   arr[j + 1] = temp;
			}	
}

int main() {
int arr[] = {2,1,9,5,3,2,1};
int len = (int)sizeof(arr)/sizeof(*arr);
bubble_sort(arr,len);
for(int i=0; i<len; i++){
 printf("%d ",arr[i]);
}
printf("length:%d", len);	
return 0;
} 
